    "properties": {
      "passengerId": { "type": "integer" },
      "passengerName": { "type": "string" },
      "passengerMobile": { "type": "string" },
      "passengerGender": { "type": "string" },
      "passengerAadhaarNumber": { "type": "string" },
      "passengerAddress": { "type": "string" }
